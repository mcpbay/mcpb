{
  "tasks": {
    "build:windows": "deno compile --allow-net --allow-read --allow-write --allow-env --target x86_64-pc-windows-msvc --output ./dist/mcpb.exe main.ts",
    "codegen": "openapi-typescript http://localhost:3000/api-json -o ./src/api/schema.d.ts",
    "start": "deno run -A --env-file=.env.mcpbay main.ts"
  },
  "imports": {
    "@mcpbay/easy-mcp-server": "jsr:@mcpbay/easy-mcp-server@^1.2.0",
    "@online/get-string-uid": "jsr:@online/get-string-uid@^1.0.1",
    "@std/assert": "jsr:@std/assert@^1.0.18",
    "@std/fs": "jsr:@std/fs@^1.0.22",
    "@std/testing": "jsr:@std/testing@^1.0.17",
    "commander": "npm:commander@^14.0.3"
  }
}